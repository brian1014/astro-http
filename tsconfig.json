{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@db": [
        "./src/db"
      ],
      "@components/*": [
        "./src/components/*"
      ]
    },
    "jsx": "preserve"
  }
}